{
  "name": "@rbxts/rbx-gc-tracker",
  "version": "1.0.0",
  "description": "GC tracker for Luau that provides more predicatable (compared to `__gc`...) destructor invocation for dead objects. Supports ~constant time update cost by limiting the iteration count such that update can be called every frame with a small n for negligible performance cost.",
  "main": "out/init.lua",
  "scripts": {
    "build": "rbxtsc",
    "watch": "rbxtsc -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": ["garbage collection", "tracking"],
  "author": "zeux",
  "license": "MIT",
  "contributors": ["HowManySmall"],
  "types": "out/index.d.ts",
  "files": [
    "out",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^2.1.1-types.0",
    "@rbxts/types": "^1.0.705",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-roblox-ts": "^0.0.35",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "roblox-ts": "^2.1.1",
    "typescript": "^5.1.6"
  }
}
